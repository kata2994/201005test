package 練習問題;


import java.util.*;

public class Main {

	public static void main(String[] args) {
		
		Hero saito = new Hero();
		Hero suzuki = new Hero();
		saito.setName("斎藤");
		suzuki.setName("鈴木");
		
		List<Hero> hN = new ArrayList<>();
		
		hN.add(saito);
		hN.add(suzuki);
		
				
		for(Hero h : hN) {
			
			System.out.println(h.getName());
			
			
		}
		
		
		
		
	}

}
